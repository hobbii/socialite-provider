{
  "name": "hobbii/socialite-provider",
  "description": "Hobbii OAuth provider for Laravel Socialite",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "laravel/socialite": "^5.4",
    "illuminate/support": "^8.81|^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "fakerphp/faker": "^1.18",
    "orchestra/testbench": "^6.0",
    "phpstan/phpstan": "^1.4",
    "friendsofphp/php-cs-fixer": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Hobbii\\SocialiteProvider\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hobbii\\SocialiteProvider\\SocialiteServiceProvider"
      ]
    }
  }
}
